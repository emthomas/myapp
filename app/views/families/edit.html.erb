<% provide(:title, "Edit Family") %>
<h1>Update <%= @family.family_name %>'s family</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@family) do |f| %>
      <%= render 'shared/error_messages_family' %>

      <%= f.label :family_name %>
      <%= f.text_field :family_name, class: 'form-control' %>
      
      <%= f.submit "Save Family.", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
